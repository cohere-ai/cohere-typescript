    "test": "npm run build && mocha -r ts-node/register test/test.ts"
  },
  "files": [
    "/dist"
  ],
  "keywords": [
    "sdk",
    "cohere",
    "cohere.ai",
    "javascript",
    "sdk",
    "api",
    "natural",
    "language"
  ],
  "author": "Cohere AI",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.18",
    "@types/mocha": "^8.2.2",
    "chai": "^4.3.4",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "^8.1.1",
    "dotenv": "^9.0.2",
    "mocha": "^8.4.0",
    "nodemon-webpack-plugin": "^4.5.2",
    "ts-loader": "^9.1.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "webpack": "^5.37.0",
    "webpack-cli": "^4.7.0"
  }
}
