{
  "name": "cohere-ai",
  "version": "1.0.0",
  "description": "A Node.js SDK for the Cohere.ai API.",
  "main": "index.js",
  "types": "dist/cohere.d.ts",
  "typings": "dist/cohere.d.ts",
  "scripts": {
    "build": "webpack --env production && mv cohere.d.ts dist/cohere.d.ts && rm services/*.d.ts && rm models/*.d.ts",
